# env/secrets/supabase.prod.env
# Production Supabase profile (use real credentials retrieved from Supabase dashboard).

ENV_PROFILE=local-with-production-supabase
ENV_SUPABASE=production

NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-production-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-production-service-role-key

# Optional direct database connection (psql, Prisma)
# SUPABASE_DB_URL=postgresql://postgres:[PASSWORD]@db.your-project-ref.supabase.co:5432/postgres
