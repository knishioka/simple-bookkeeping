{
  "name": "@simple-bookkeeping/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${WEB_PORT:-3000}",
    "build": "NODE_ENV=production next build",
    "start": "next start -p ${WEB_PORT:-3000}",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:fast": "TEST_MODE=fast playwright test",
    "test:e2e:ci": "TEST_MODE=ci playwright test --reporter=list",
    "test:e2e:comprehensive": "TEST_MODE=comprehensive playwright test",
    "test:e2e:local": "TEST_MODE=local playwright test",
    "test:e2e:prod": "TEST_MODE=prod playwright test",
    "test:e2e:basic": "./e2e-run-basic.sh",
    "test:e2e:docker": "../../scripts/test-runner.sh e2e",
    "test:e2e:docker:headed": "../../scripts/test-runner.sh e2e --headed",
    "test:e2e:docker:auth": "../../scripts/test-runner.sh e2e --test auth-test.spec.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next .turbo node_modules",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@simple-bookkeeping/config": "workspace:*",
    "@simple-bookkeeping/database": "workspace:*",
    "@simple-bookkeeping/shared": "workspace:*",
    "@simple-bookkeeping/supabase-client": "workspace:*",
    "@simple-bookkeeping/test-utils": "workspace:*",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.84.1",
    "@types/csv-parse": "^1.2.5",
    "@types/jsonwebtoken": "^9.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.536.0",
    "next": "15.5.2",
    "react": "^19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "wanakana": "^5.3.1",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.54.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@simple-bookkeeping/typescript-config": "workspace:*",
    "@size-limit/preset-app": "^11.2.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "eslint-import-resolver-typescript": "3.6.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "size-limit": "^11.2.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  },
  "size-limit": [
    {
      "name": "Main Bundle",
      "path": ".next/static/chunks/pages/*.js",
      "limit": "250 KB"
    },
    {
      "name": "First Load JS",
      "path": ".next/static/chunks/main*.js",
      "limit": "100 KB"
    }
  ]
}
